{
  "name": "@nestjs-addons/in-memory-db",
  "version": "0.0.0-development",
  "description": "Simple In-Memory DB Service for NestJS projects",
  "author": "Wes Grimes",
  "license": "MIT",
  "repository": "https://github.com/nestjs-addons/in-memory-db",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "build:dev": "tsc-watch -p tsconfig.json",
    "format": "prettier lib/**/*.ts --write",
    "format:verify": "prettier lib/**/*.ts --check",
    "lint": "npx tslint -p tsconfig.json -c tslint.json \"lib/**/*.ts\" -e \"*.spec.ts\" --force",
    "test": "jest --config jest.json",
    "semantic-release": "semantic-release",
    "commit": "npx git-cz"
  },
  "devDependencies": {
    "@nestjs/common": "6.5.3",
    "@nestjs/core": "6.5.3",
    "@semantic-release/changelog": "3.0.4",
    "@types/jest": "24.0.18",
    "@types/node": "11.13.19",
    "cz-conventional-changelog": "3.0.2",
    "husky": "3.0.4",
    "jest": "24.9.0",
    "lint-staged": "9.2.4",
    "prettier": "1.18.2",
    "reflect-metadata": "0.1.13",
    "rxjs-marbles": "5.0.2",
    "semantic-release": "15.13.24",
    "ts-jest": "24.0.2",
    "tsc-watch": "2.4.0",
    "typescript": "3.5.3",
    "tslint": "5.19.0"
  },
  "peerDependencies": {
    "@nestjs/common": "^6.0.0",
    "@nestjs/core": "^6.0.0",
    "reflect-metadata": "^0.1.12"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "git add -f"
    ],
    "*.ts,!*.spec.ts": [
      "npm run lint"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/github"
    ]
  },
  "dependencies": {}
}
